    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "i18next": "^22.4.9",
    "react": "18.2.0",
    "react-i18next": "^12.1.5",
    "react-native": "0.71.2",
    "react-native-bootsplash": "^4.5.0",
    "react-native-easy-icon": "^1.0.10",
