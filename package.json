    "ios": "react-native run-ios",
    "lint": "npx eslint --ext .jsx,.js,.tsx,.ts src/",
    "start": "react-native start",
    "test": "jest",
    "prepare": "husky install",
    "format:check": "prettier --check ./src",
    "format:fix": "npx prettier --write ./src"
  },
